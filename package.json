{
  "name": "mytube",
  "version": "1.0.0",
  "description": "MyTube",
  "author": "itukikikuti",
  "main": "dist/main.js",
  "scripts": {
    "start": "cross-env NODE_OPTIONS='--openssl-legacy-provider' NODE_ENV=development webpack --progress && electron .",
